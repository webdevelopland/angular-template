{
  "name": "angular-default",
  "version": "2.6.0",
  "description": "Pattern for new angular projects",
  "scripts": {
    "start": "node express.js",
    "serve": "ng serve --proxy-config front-end/dev/express.proxy.json",
    "prod": "ng build --prod --aot",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "scss": "gulp scss",
    "scss-auto": "gulp scss-auto",
    "express": "gulp express-auto",
    "unit": "npm-run-all --parallel scss-auto express test",
    "dev": "npm-run-all --parallel scss-auto serve",
    "devx": "npm-run-all --parallel express scss-auto serve",
    "build": "npm-run-all scss prod",
    "buildx": "npm-run-all scss prod express"
  },
  "author": {
    "name": "Web Developland",
    "email": "webdevelopland@protonmail.com"
  },
  "license": "MIT",
